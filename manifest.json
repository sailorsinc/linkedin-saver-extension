{
  "manifest_version": 3,
  "name": "LinkedIn Profile Saver",
  "description": "Save your Friend's LinkedIn profile  as Resume and Refer",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}